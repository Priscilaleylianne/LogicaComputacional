Algoritmo "CalculoMédiaAritimética"
Var
   N1,N2,N3,N4,MA:Real
Inicio
        ESCREVAL("DIGITE NOTA N1: ")
        LEIA(N1)
        ESCREVAL("DIGITE NOTA N2: ")
        LEIA(N2)
        ESCREVAL("DIGITE NOTA N3: ")
        LEIA(N3)
        ESCREVAL("DIGITE NOTA N4: ")
        LEIA(N4)
        MA<-(N1+N2+N3+N4)/4
        ESCREVAL("MEDIA FINAL ANUAL = ", MA)
Fimalgoritmo
