algoritmo "InformacoesPessoais"
var
   nome, cidade: caractere
   idade: inteiro
inicio
   escreva("Digite seu nome: ")
   leia(nome)

   escreva("Digite sua idade: ")
   leia(idade)

   escreva("Digite o nome da cidade onde mora: ")
   leia(cidade)

   escreval("Olá, meu nome é ", nome, ", tenho ", idade, " anos e moro em ", cidade, ".")
fimalgoritmo
